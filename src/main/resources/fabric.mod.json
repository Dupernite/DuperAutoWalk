{
  "schemaVersion": 1,
  "id": "duperautowalk",
  "version": "${version}",
  "name": "Duper's AutoWalk",
  "description": "A simple mod that allows you to walk without holding the forward key.",
  "authors": [],
  "contact": {
    "repo": "https://github.com/Dupernite/DuperAutoWalk"
  },
  "license": "MIT",
  "icon": "assets/duperautowalk/icon.png",
  "environment": "client",
  "entrypoints": {
    "client": [ "com.dupernite.duperautowalk.client.DuperAutoWalkClient" ],
    "main": [ "com.dupernite.duperautowalk.DuperAutoWalk" ],
    "modmenu": [ "com.dupernite.duperautowalk.compat.ModMenuAPI" ]
  },
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric": "*",
    "minecraft": "${minecraft_version}"
  },
  "custom": {
    "modmenu": {
      "links": {
        "modmenu.modrinth": "https://modrinth.com/project/autowalker"
      },
      "update_checker": true
    }
  }
}
